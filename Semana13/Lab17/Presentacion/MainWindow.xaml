<Window x:Class="Presentacion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentacion"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnVerDocentes" Content="Ver Docentes" HorizontalAlignment="Center" Margin="0,87,0,0" VerticalAlignment="Top" Width="116" Click="btnVerDocentes_Click"/>
        <Button x:Name="btnVerCursos" Content="Ver Cursos" HorizontalAlignment="Center" Margin="0,112,0,0" VerticalAlignment="Top" Width="116" Click="btnVerCursos_Click"/>
        <Button x:Name="btnAsignar" Content="Asignar" HorizontalAlignment="Center" Margin="0,137,0,0" VerticalAlignment="Top" Width="116" Click="btnAsignar_Click"/>
        <Button x:Name="btnSalir" Content="Salir" HorizontalAlignment="Center" Margin="0,162,0,0" VerticalAlignment="Top" Width="116" Click="btnSalir_Click"/>
    </Grid>
</Window>
